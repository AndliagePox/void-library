<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.andliage.library.entity.Hold" table="hold" schema="library">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <many-to-one name="user" class="com.andliage.library.entity.User">
            <column name="user" not-null="true"/>
        </many-to-one>
        <many-to-one name="book" class="com.andliage.library.entity.Book">
            <column name="book" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>